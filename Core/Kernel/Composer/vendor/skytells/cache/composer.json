{
    "name": "skytells/cache",
    "description": "The Skytells Cache package.",
    "license": "MIT",
    "require": {
        "php": ">=5.6.4",
        "skytells/contracts": "1.*",
        "skytells/support": "1.*",
        "nesbot/carbon": "~1.20"
    },
    "autoload": {
        "psr-4": {
            "Skytells\\Cache\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "suggest": {
        "skytells/database": "Required to use the database cache driver (1.*).",
        "skytells/filesystem": "Required to use the file cache driver (1.*).",
        "skytells/redis": "Required to use the redis cache driver (1.*)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
